/******************************************************************************

Exercicio que conta quantos numeros em um vetor s√£o iguais.
*******************************************************************************/
#include <stdio.h>
int main()
{
    int v1[3];
    int v2[3];
    int cont=0;
    
    
    for(int i=0;i<3;i++){
        printf("Digite um numero: \n");
        scanf("%d", &v1[i]);
    for(int j=0;j<3;j++){
        printf("Digite um numero: \n");
        scanf("%d", &v2[j]);
    }
    for(int i=0;i<3;i++){
        for(int j=0;j<3;j++){
            if(v1[i] == v2[j]){
                cont++;
            }
        }
    }
    }
    printf("Foram repetidos %dx", cont);
}
